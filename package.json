{
	"name": "obsidian-inbox",
	"version": "3.0.0",
	"description": "Show in app notification if there is data to process in the \"inbox\" note/folder.",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"test": "vitest",
		"test:ci": "vitest run",
		"version": "npx ts-node --esm external/obsidian-plugin-scripts/version-bump.mts && git add package.json package-lock.json manifest.json versions.json"
	},
	"keywords": [
		"obsidian-plugin"
	],
	"author": "Zachatoo",
	"license": "MIT",
	"devDependencies": {
		"@tsconfig/svelte": "^3.0.0",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.14.47",
		"esbuild-svelte": "^0.7.3",
		"eslint-plugin-svelte3": "^4.0.0",
		"obsidian": "latest",
		"svelte": "^3.55.1",
		"svelte-preprocess": "^5.0.1",
		"tslib": "2.4.0",
		"typescript": "^4.9.0",
		"vitest": "^0.34.4"
	},
	"dependencies": {
		"@popperjs/core": "^2.11.6",
		"obsidian-svelte": "^0.1.10",
		"svelte-portal": "^2.2.0"
	}
}
